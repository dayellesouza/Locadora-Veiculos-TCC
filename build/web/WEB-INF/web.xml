<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>Autenticador</servlet-name>
        <servlet-class>controle.Autenticador</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ControleCliente</servlet-name>
        <servlet-class>controle.ControleCliente</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ControleServico</servlet-name>
        <servlet-class>controle.ControleServico</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ControleUsuario</servlet-name>
        <servlet-class>controle.ControleUsuario</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>controleAgendamento</servlet-name>
        <servlet-class>controle.controleAgendamento</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ControleLogin</servlet-name>
        <servlet-class>controle.ControleLogin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ControleAcesso</servlet-name>
        <servlet-class>controle.ControleAcesso</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ControleCarro</servlet-name>
        <servlet-class>controle.ControleCarro</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Autenticador</servlet-name>
        <url-pattern>/Autenticador</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ControleCliente</servlet-name>
        <url-pattern>/ControleCliente</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ControleServico</servlet-name>
        <url-pattern>/ControleServico</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ControleUsuario</servlet-name>
        <url-pattern>/ControleUsuario</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>controleAgendamento</servlet-name>
        <url-pattern>/controleAgendamento</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ControleLogin</servlet-name>
        <url-pattern>/ControleLogin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ControleAcesso</servlet-name>
        <url-pattern>/ControleAcesso</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ControleCarro</servlet-name>
        <url-pattern>/ControleCarro</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
